    <UserSecretsId>2463713c-7d4c-4c07-958c-f7497769caae</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
	  <PackageReference Include="Microsoft.EntityFrameworkCore" Version="8.0.0" />
